AAM,CAACa,UAAU,CAAG,EAAd,EAAoBD,OAA1B,CACA,mBACE,oBAAC,MAAD,EACE,KAAK,CAAG,GAAEC,UAAU,CAACT,KAAM,EAD7B,CAEE,WAAW,CAAC,iDAFd,eAGE,8BAAQ,SAAS,CAAEP,IAAI,CAAC,oBAAD,CAAuBK,MAAM,CAACY,UAA9B,CAAvB,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,yBAAG,SAAS,CAAC,gBAAb,EAA+BD,UAAU,CAACE,OAA1C,CADF,cAEE,2BAAK,SAAS,CAAEb,MAAM,CAACc,OAAvB,eACE,oBAAC,IAAD,EACE,SAAS,CAAEnB,IAAI,CACb,qDADa,CAEbK,MAAM,CAACe,UAFM,CADjB,CAKE,EAAE,CAAEhB,UAAU,CAAC,kBAAD,CALhB,gBADF,CAFF,CADF,CAHF,cAkBE,gCACGE,QAAQ,EAAIA,QAAQ,CAACe,MAAT,CAAkB,CAA9B,eACC,+BAAS,SAAS,CAAEhB,MAAM,CAACC,QAA3B,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,2BAAK,SAAS,CAAC,KAAf,EACGA,QAAQ,CAACgB,GAAT,CAAa,CAACC,KAAD,CAAQC,GAAR,gBACZ,oBAAC,OAAD,WAAS,GAAG,CAAEA,GAAd,EAAuBD,KAAvB,EADD,CADH,CADF,CADF,CAFJ,CAlBF,CADF,CAkCD,CAED,cAAeT,CAAAA,IAAf","sourcesContent":["import React from 'react';\nimport clsx from 'clsx';\nimport Layout from '@theme/Layout';\nimport Link from '@docusaurus/Link';\nimport useDocusaurusContext from '@docusaurus/useDocusaurusContext';\nimport useBaseUrl from '@docusaurus/useBaseUrl';\nimport styles from './styles.module.css';\n\nconst features = [\n  {\n    title: 'Compare 16S variability',\n    imageUrl: 'img/undraw_version_control_9bpv.svg',\n    description: (\n      <>\n        rRNADif will automatically annotate 16S in your genome and compare them \n        to precomputed database of 21000+ fully sequenced Bacteria genomes. (or \n        it's subset)\n      </>\n    ),\n  },\n  {\n    title: 'Build species phylogenetic trees',\n    imageUrl: 'img/undraw_Tree_swing_646s.svg',\n    description: (\n      <>\n        rRNADif lets you not only to measure degree of heterogenuity of 16S rRNA,\n        but also use that information to see to what extend topology of the phylogenetic\n        tree can be altered.\n      </>\n    ),\n  },\n  {\n    title: 'Build custom database',\n    imageUrl: 'img/undraw_server_status_5pbv.svg',\n    description: (\n      <>\n        Not happy with provided data? No worries! Built you own databases using\n        csv file from GenBank or locally stored genome sequences.\n      </>\n    ),\n  },\n];\n\nfunction Feature({imageUrl, title, description}) {\n  const imgUrl = useBaseUrl(imageUrl);\n  return (\n    <div className={clsx('col col--4', styles.feature)}>\n      {imgUrl && (\n        <div className=\"text--center\">\n          <img className={styles.featureImage} src={imgUrl} alt={title} />\n        </div>\n      )}\n      <h3>{title}</h3>\n      <p>{description}</p>\n    </div>\n  );\n}\n\nfunction Home() {\n  const context = useDocusaurusContext();\n  const {siteConfig = {}} = context;\n  return (\n    <Layout\n      title={`${siteConfig.title}`}\n      description=\"Description will go into a meta tag in <head />\">\n      <header className={clsx('hero hero--primary', styles.heroBanner)}>\n        <div className=\"container\">\n          <p className=\"hero__subtitle\">{siteConfig.tagline}</p>\n          <div className={styles.buttons}>\n            <Link\n              className={clsx(\n                'button button--outline button--secondary button--lg',\n                styles.getStarted,\n              )}\n              to={useBaseUrl('docs/get_started')}>\n              Get Started\n            </Link>\n          </div>\n        </div>\n      </header>\n      <main>\n        {features && features.length > 0 && (\n          <section className={styles.features}>\n            <div className=\"container\">\n              <div className=\"row\">\n                {features.map((props, idx) => (\n                  <Feature key={idx} {...props} />\n                ))}\n              </div>\n            </div>\n          </section>\n        )}\n      </main>\n    </Layout>\n  );\n}\n\nexport default Home;\n"]}]}